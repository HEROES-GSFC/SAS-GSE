<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Start Saving Images</key>
	<dict>
		<key>var_names</key>
		<array>
			<string>value</string>
		</array>
		<key>description</key>
		<string>Image (FITS) file saving will be turned on</string>
		<key>key</key>
		<string>0x0100</string>
	</dict>
	<key>Stop Saving Images</key>
	<dict>
		<key>var_names</key>
		<array>
			<string>value</string>
		</array>
		<key>description</key>
		<string>Image (FITS) file saving will be turned off</string>
		<key>key</key>
		<string>0x0100</string>
	</dict>
	<key>Set Preamp gain</key>
	<dict>
		<key>var_names</key>
		<array>
			<string>value</string>
		</array>
		<key>description</key>
		<string>Set the preamp gain value</string>
		<key>key</key>
		<string>0x0191</string>
	</dict>
	<key>Set Analog gain</key>
	<dict>
		<key>var_names</key>
		<array>
			<string>value</string>
		</array>
		<key>description</key>
		<string>Set the analog gain value  (log scale, ~160 doubles the gain)</string>
		<key>key</key>
		<string>0x0181</string>
	</dict>
	<key>Set exposure time</key>
	<dict>
		<key>var_names</key>
		<array>
			<string>time (us)</string>
		</array>
		<key>description</key>
		<string>Set exposure time, t in microseconds</string>
		<key>key</key>
		<string>0x0151</string>
	</dict>
	<key>Set new pointing</key>
	<dict>
		<key>key</key>
		<string>0x0112</string>
		<key>description</key>
		<string>Set new solar target, x and y in arcseconds</string>
		<key>var_names</key>
		<array>
			<string>x</string>
			<string>y</string>
		</array>
	</dict>
	<key>Request PYAS RAW Image</key>
	<dict>
		<key>var_names</key>
		<array/>
		<key>description</key>
		<string>Ask for a full frame to be sent over TCP</string>
		<key>key</key>
		<string>0x0210</string>
	</dict>
	<key>Restart all Workers</key>
	<dict>
		<key>var_names</key>
		<array/>
		<key>description</key>
		<string>Kill and then restart all workers</string>
		<key>key</key>
		<string>0x0020</string>
	</dict>
	<key>Test</key>
	<dict>
		<key>var_names</key>
		<array/>
		<key>key</key>
		<string>0x1000</string>
		<key>description</key>
		<string>send a test command (does nothing)</string>
	</dict>
	<key>Kill all Workers</key>
	<dict>
		<key>var_names</key>
		<array/>
		<key>description</key>
		<string>Kill all worker thread (except command listening thread)</string>
		<key>key</key>
		<string>0x0010</string>
	</dict>
</dict>
</plist>
